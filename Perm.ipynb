    "from itertools import permutations\n",
    "\n",
    "def list_of_perm(n):\n",
    "    perm = permutations(range(1, n+1))\n",
    "    for p in perm:\n",
    "        print (*p)\n",
    "\n",
    "n = int(input())\n",
    "\n",
    "perm = permutations(range(1, n+1))\n",
    "np = len(list(perm))\n",
    "print (np)\n",
    "list_of_perm(n)"
